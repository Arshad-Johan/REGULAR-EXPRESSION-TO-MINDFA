// DFA Representation
digraph {
	entryHidden [label="" style=invis]
	2 [label=2 shape=circle]
	3 [label=3 shape=circle]
	1 [label=1 shape=circle]
	0 [label=0 shape=doublecircle]
	2 -> 3 [label=a]
	3 -> 1 [label=b]
	1 -> 1 [label=c]
	1 -> 1 [label=b]
	1 -> 0 [label=d]
	0 -> 0 [label=d]
	entryHidden -> 2
}
