// DFA Representation
digraph {
	entryHidden [label="" style=invis]
	0 [label=0 shape=circle]
	1 [label=1 shape=circle]
	2 [label=2 shape=circle]
	3 [label=3 shape=circle]
	4 [label=4 shape=doublecircle]
	5 [label=5 shape=circle]
	0 -> 1 [label=a]
	1 -> 2 [label=b]
	2 -> 3 [label=b]
	2 -> 4 [label=d]
	2 -> 5 [label=c]
	3 -> 3 [label=b]
	3 -> 4 [label=d]
	3 -> 5 [label=c]
	4 -> 4 [label=d]
	5 -> 3 [label=b]
	5 -> 4 [label=d]
	5 -> 5 [label=c]
	entryHidden -> 0
}
